    "minimum-stability": "stable",
    "require": {},
    "require-dev": {
        "squizlabs/php_codesniffer": "^3.4",
        "phpmd/phpmd": "^2.7",
        "phpunit/phpunit": "^8.3",
        "slevomat/coding-standard": "^5.0"
