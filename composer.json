    "autoload-dev": {
        "psr-4": {
            "Core\\": "./Core/src/"
        }
    },
    "scripts": {
        "test": "phpunit",
        "cs": "phpcs",
        "fix": "phpcbf",
        "md": "phpmd Core text cleancode, codesize, controversial, design, naming, unusedcode",
